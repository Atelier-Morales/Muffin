<script>

        window.toCheck = [{{ checkedRadios }}];
    </script>
    <script>

        $(document).ready(function()
        {
            $(".footer-container").addClass("nofix");
            NProgress.start();

            $("[data-submit]").each(function() {
                $(this).click(function() {
                    $('#' + $(this).attr("data-submit")).trigger('submit');
                });
            });
            $('li[data-index]').hide();
            $('li[data-index="1"]').show();
            initalizeForm();
            initalizeAddForm();
            addCheckHandler(window.toCheck);
            NProgress.done();
            treatResize();

        });
    </script>